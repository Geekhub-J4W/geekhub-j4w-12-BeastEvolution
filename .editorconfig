root = true

[*]
insert_final_newline = true
trim_trailing_whitespace = true
end_of_line = lf

[*.java]
charset = utf-8
indent_style = space
indent_size = 4

[*.csv]
charset = utf-8
insert_final_newline = false
trim_trailing_whitespace = false

[*.txt]
charset = utf-8
insert_final_newline = false
trim_trailing_whitespace = false

[*.properties]
charset = utf-8
insert_final_newline = false
trim_trailing_whitespace = false
